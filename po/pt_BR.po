msgstr "ACROS + Ye Filtro"

#: src/sonymn_int.cpp:1607
msgid "ADI"
msgstr "ADI"

#: src/minoltamn_int.cpp:238
msgid "ADI (Advanced Distance Integration)"
msgstr "ADI (Integração Avançada Distância)"

#: src/casiomn_int.cpp:202
msgid "AE Bracketing"
msgstr "Assistente de auto-exposição"

#: src/minoltamn_int.cpp:1004 src/olympusmn_int.cpp:468
msgid "AE Lock"
msgstr "Bloqueio AE"

#: src/nikonmn_int.cpp:406
msgid "AE bracket compensation applied"
msgstr "Compensação de nível AE aplicada"

