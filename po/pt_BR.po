msgid "             i : IPTC tags\n"
msgstr "             i : Tags IPTC\n"

#: app/exiv2.cpp:289
msgid "             i : IPTC tags (-PIkyct)\n"
msgstr "             i : Tags IPTC (-PIkyct)\n"

