msgstr "A-DEP"

#: src/canonmn_int.cpp:2003
msgid "A-TTL"
msgstr "A-TTL"

#: src/properties.cpp:120
msgid "ACDSee XMP schema"
msgstr "ACDSee esquema XMP"

#: src/fujimn_int.cpp:65
msgid "ACROS"
msgstr "ACROS"

#: src/fujimn_int.cpp:68
msgid "ACROS + G Filter"
msgstr "ACROS + Filtro G"

#: src/fujimn_int.cpp:66
msgid "ACROS + R Filter"
msgstr "ACROS + R Filtro"

#: src/fujimn_int.cpp:67
