msgid "             X : XMP sidecar from file <file>.xmp\n"
msgstr "             X : XMP sidecar del archivo <file>.xmp\n"

#: app/exiv2.cpp:328
msgid "             X : XMP sidecar to <file>.xmp\n"
msgstr "             X : XMP sidecar a <file>.xmp\n"

#: app/exiv2.cpp:297
msgid "             X : XMP tags\n"
msgstr "             X: Etiquetas XMP\n"

#: app/exiv2.cpp:321
msgid ""
"             XX: \"raw\" metadata from <file>.exv. XMP default, optional "
"Exif and IPTC\n"
msgstr ""
"             XX: metadatos \"raw\" de <file>.exv. XMP por defecto, Exif "
"opcional e IPTC\n"

#: app/exiv2.cpp:329
msgid ""
"             XX: \"raw\" metadata to <file>.exv. XMP default, optional Exif "
