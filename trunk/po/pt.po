msgstr "A"

#: src/tags.cpp:349
msgid "B"
msgstr "B"

#: src/tags.cpp:350
msgid "C"
msgstr "C"

#: src/tags.cpp:351
msgid "A+B-C"
msgstr "A+B-C"

#: src/tags.cpp:352
