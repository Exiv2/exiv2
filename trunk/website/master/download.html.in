  function swapcols() {
    if ($(window).width() < 768 && !window.swapped) {
      $(".col-sm-pair").each( function() {
        $(".col-sm-6", this).eq(0).after($(".col-sm-6", this).eq(2).remove());
      });
      window.swapped = true;
    }
    if ($(window).width() >= 768 && window.swapped) {
      $(".col-sm-pair").each( function() {
        $(".col-sm-6", this).eq(3).before($(".col-sm-6", this).eq(1).remove());
      });
      window.swapped = false;
    }
  }
  //]]>
</script>
